	"type": "module",
	"license": "GNU General Public License v3.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/EgeOnder/kafeasist.git"
	},
	"scripts": {
		"build": "turbo build",
		"build:www": "turbo --filter=www build",
		"build:api": "turbo --filter=api build",
		"build:pkg": "turbo --filter='./packages/*' build",
		"start": "turbo start",
		"start:www": "turbo --filter=www start",
		"start:api": "turbo --filter=api start",
		"dev": "turbo dev",
		"dev:www": "turbo --filter=www dev",
		"dev:api": "turbo --filter=api dev",
		"lint": "turbo lint",
		"format": "turbo format && prettier --write \"**/*.{ts,tsx,md}\"",
		"release": "changeset version",
		"manypkg:check": "manypkg check",
		"manypkg:fix": "manypkg fix",
		"test": "turbo test",
		"test:api": "turbo --filter=api test",
		"test:pkg": "turbo --filter='./packages/*' test",
		"check": "pnpm lint && pnpm format && pnpm manypkg:fix && pnpm test"
	},
	"engines": {
		"node": ">=14.0.0"
	},
	"dependencies": {
		"@changesets/cli": "^2.26.0",
		"@kafeasist/api": "1.1.0",
		"@kafeasist/auth": "1.0.0",
		"@kafeasist/responses": "2.0.0",
		"@manypkg/cli": "^0.20.0",
		"prettier": "latest",
		"turbo": "latest"
	},
	"packageManager": "pnpm@7.27.0"
}
